{
  "name": "neukit",
  "version": "0.1.0",
  "description": "Neutralino CLI tools",
  "bin": {
    "neukit": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "bun run ./bin/index.ts",
    "build": "bun build ./bin/index.ts --outdir ./dist --target node",
    "prepublishOnly": "bun run build",
    "release:patch": "npm version patch && git push --follow-tags && npm publish",
    "release:minor": "npm version minor && git push --follow-tags && npm publish",
    "releas:emajor": "npm version major && git push --follow-tags && npm publish"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "5"
  },
  "dependencies": {
    "8colors": "1.0.2",
    "@types/degit": "2.8.6",
    "@types/figlet": "1.7.0",
    "commander": "14.0.0",
    "degit": "2.8.4",
    "enquirer": "2.4.1",
    "figlet": "1.8.1",
    "ora": "8.2.0",
    "png2icons": "2.0.1"
  }
}
